{
  "_comment": "Paths are relative to cdawmeta.DATA_DIR",
  "default": {
    "name": "default",
    "file_log": "{dir_name}/{name}.log",
    "file_error": "{dir_name}/{name}.error.log",
    "console_format": "%(name)s %(levelname)s [%(threadName)s] %(message)s",
    "console_format_debug": "%(name)s %(levelname)s %(filename)s:%(lineno)d %(message)s",
    "color": true,
    "rm_string": " [MainThread]"
  },
  "metadata": {
    "name": "metadata",
    "file_log": "{name}.log",
    "file_error": false
  },
  "hapi": {
    "name": "hapi",
    "file_format": "%(name)s %(levelname)s [%(threadName)s] %(message)s",
    "file_log": "{name}/{name}.log",
    "file_error": false
  },
  "master_resolved": {
    "name": "master_resolved",
    "file_format": "%(name)s %(levelname)s [%(threadName)s] %(message)s",
    "file_log": "{name}/{name}.log",
    "file_error": false
  }
}
